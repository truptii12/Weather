<p id="notice"><%= notice %></p>

<h1>Listing Temps</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th>Min</th>
      <th>Max</th>
      <th>Night</th>
      <th>Eve</th>
      <th>Morn</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temps.each do |temp| %>
      <tr>
        <td><%= temp.day %></td>
        <td><%= temp.min %></td>
        <td><%= temp.max %></td>
        <td><%= temp.night %></td>
        <td><%= temp.eve %></td>
        <td><%= temp.morn %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>

